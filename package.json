{
  "name": "imicros-events",
  "version": "0.0.2",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Moleculer service for persistent event handling",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache --config=travis.js",
    "test-local": "jest --coverage --no-cache --config=local.js",
    "test-admin": "jest test/events.admin.js --no-cache --config=local.js",
    "test-sub": "jest test/events.subscriber.spec.js --no-cache --config=local.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-events.git"
  },
  "keywords": [
    "moleculer",
    "events"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "jest": "^24.8.0",
    "moleculer": "^0.13.9",
    "nats": "^1.3.0"
  },
  "dependencies": {
    "kafkajs": "^1.8.0",
    "lodash": "^4.17.11",
    "uuidv4": "^3.0.1"
  }
}
