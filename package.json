{
  "name": "imicros-events",
  "version": "0.0.1",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Moleculer service for persistent event handling",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache --config=travis.js",
    "test-local": "jest --coverage --no-cache --config=local.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-events.git"
  },
  "keywords": [
    "moleculer",
    "events"
  ],
  "devDependencies": {
    "coveralls": "^3.0.3",
    "jest": "^24.3.1",
    "moleculer": "^0.13.7",
    "nats": "^1.2.2"
  },
  "dependencies": {
    "kafkajs": "^1.5.0",
    "lodash": "^4.17.11",
    "uuidv4": "^3.0.1"
  }
}
